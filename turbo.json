{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "cache": false,
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "preview": {
      "dependsOn": ["^preview"]
    },
    "check": {
      "dependsOn": ["^check"]
    },
    "fix": {
      "dependsOn": ["^fix"]
    }
  }
}
